{
    "family": "collarks",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::044462970062:role/ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": ["FARGATE"],
    "containerDefinitions": [
        {
            "name": "collarks",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "protocol": "tcp",
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
